<% include partials/header%>

    

<div class="container">
    <header class="jumbotron">
        <div class="container">
            
       
        <h1>View all campgrounds</h1>
        <p>Campgrounds from all over the world</p>
        <a class="btn btn-primary btn-large" href="/campgrounds/new">Add campground</a>
     </div>
    </header>
<div class="row text-center">
    <%campgrounds.forEach(function(campground){ %>
  <div class="col-md-3 col-sm-6">
      <div class="thumbnail">
          
       <img hspace="3px" src="<%=campground.image%>" >
      </div>
      <div class="caption"> <h4> <%=campground.name%> </h4> </div>
         <a class="btn btn-primary" href="/campgrounds/<%=campground._id%>">More Info</a>
  </div>  
<%});%>
</div>

</div>

<% include partials/footer%>
